	<queryString>
		<![CDATA[SELECT
     hoadon.`Mã Hóa Đơn` AS hoadon_Mã_Hóa_Đơn,
     hoadon.`Mã Sản Phẩm` AS hoadon_Mã_Sản_Phẩm,
     hoadon.`Số Lượng` AS hoadon_Số_Lượng,
     hoadon.`Đơn Giá` AS hoadon_Đơn_Giá,
     hoadon.`Thành Tiền` AS hoadon_Thành_Tiền,
     hoadon.`Personid` AS hoadon_Personid,
     khachhang.`Tên Khách Hàng` AS khachhang_Tên_Khách_Hàng,
     khachhang.`Địa Chỉ` AS khachhang_Địa_Chỉ,
     khachhang.`SĐT` AS khachhang_SĐT,
     khachhang.`Email` AS khachhang_Email,
     khachhang.`Đơn Hàng` AS khachhang_Đơn_Hàng,
     khachhang.`Điểm Tích Lũy` AS khachhang_Điểm_Tích_Lũy,
     khachhang.`id` AS khachhang_id,
     sanpham.`Mã Sản Phẩm` AS sanpham_Mã_Sản_Phẩm,
     sanpham.`Tên Sản Phẩm` AS sanpham_Tên_Sản_Phẩm,
     sanpham.`Loại` AS sanpham_Loại,
     sanpham.`Số Lượng` AS sanpham_Số_Lượng,
     sanpham.`Giá Tiền` AS sanpham_Giá_Tiền,
     sanpham.`Ghi Chú` AS sanpham_Ghi_Chú,
     danhsachhoadon.`Mã Hóa Đơn` AS danhsachhoadon_Mã_Hóa_Đơn,
     danhsachhoadon.`Tên Khách Hàng` AS danhsachhoadon_Tên_Khách_Hàng,
     danhsachhoadon.`Ngày Lập` AS danhsachhoadon_Ngày_Lập,
     danhsachhoadon.`Tình Trạng` AS danhsachhoadon_Tình_Trạng,
     danhsachhoadon.`Ghi Chú` AS danhsachhoadon_Ghi_Chú,
     nhanvien.`Mã Nhân Viên` AS nhanvien_Mã_Nhân_Viên,
     nhanvien.`Tên Nhân Viên` AS nhanvien_Tên_Nhân_Viên,
     nhanvien.`Giới Tính` AS nhanvien_Giới_Tính,
     nhanvien.`Ngày Sinh` AS nhanvien_Ngày_Sinh,
     nhanvien.`SĐT` AS nhanvien_SĐT,
     nhanvien.`Địa Chỉ` AS nhanvien_Địa_Chỉ
FROM
     `sanpham` sanpham INNER JOIN `hoadon` hoadon ON sanpham.`Mã Sản Phẩm` = hoadon.`Mã Sản Phẩm`
     INNER JOIN `danhsachhoadon` danhsachhoadon ON hoadon.`Mã Hóa Đơn` = danhsachhoadon.`Mã Hóa Đơn`
     INNER JOIN `khachhang` khachhang ON danhsachhoadon.`Tên Khách Hàng` = khachhang.`Tên Khách Hàng`,
     `nhanvien` nhanvien]]>
	</queryString>
